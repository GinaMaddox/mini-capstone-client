<a href= "/client/products/new">Make a New Product</a>

<% @product.each do |product|%>
  <p>id: <%= product['id'] %></p>
  <p>name: <%= product['name'] %></p>
  <p>price: <%= product['price'] %></p>
  <p>description: <%= product['description'] %></p>
  <p>image_url: <%= product['image_url'] %></p>
  <hr>
<% end %>