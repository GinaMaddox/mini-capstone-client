<% @carted_products.each do |carted_product|%>
  <p><%= carted_product['id'] %></p>
  <p><%= carted_product['quantity'] %>
  <p><%= carted_product['product']['name'] %></p>
  <p><%= carted_product['product']['price'] %></p>
  <hr>
<% end %>

<!-- form that allows user to buy items -->
<a href="/client/orders" data-method="post">Buy Now</a>